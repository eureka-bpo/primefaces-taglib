<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
        version="2.0">

    <name>eurekabpo_primefaces</name>

    <component>
        <component-type>eu.eurekabpo.primefaces.component.premiumschedule.PremiumSchedule</component-type>
        <component-class>eu.eurekabpo.primefaces.component.premiumschedule.PremiumSchedule</component-class>
    </component>

    <component>
        <component-type>eu.eurekabpo.primefaces.component.inputtext.InputText</component-type>
        <component-class>eu.eurekabpo.primefaces.component.inputtext.InputText</component-class>
    </component>

    <component>
        <component-type>eu.eurekabpo.primefaces.component.inputtextarea.InputTextarea</component-type>
        <component-class>eu.eurekabpo.primefaces.component.inputtextarea.InputTextarea</component-class>
    </component>

    <component>
        <component-type>eu.eurekabpo.primefaces.component.schedule.CronExpressionEditor</component-type>
        <component-class>eu.eurekabpo.primefaces.component.schedule.CronExpressionEditor</component-class>
    </component>

    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>eu.eurekabpo.primefaces.component.premiumschedule.PremiumSchedule</renderer-type>
            <renderer-class>eu.eurekabpo.primefaces.component.premiumschedule.PremiumScheduleRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.InputTextRenderer</renderer-type>
            <renderer-class>org.primefaces.component.inputtext.InputTextRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.InputTextareaRenderer</renderer-type>
            <renderer-class>org.primefaces.component.inputtextarea.InputTextareaRenderer</renderer-class>
        </renderer>
        <client-behavior-renderer>
            <client-behavior-renderer-type>org.primefaces.component.AjaxBehaviorRenderer</client-behavior-renderer-type>
            <client-behavior-renderer-class>org.primefaces.behavior.ajax.AjaxBehaviorRenderer</client-behavior-renderer-class>
        </client-behavior-renderer>
    </render-kit>

</faces-config>
